{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "./src/server",
  "scripts": {
    "start": "node ./src/server",
    "dev": "nodemon ./src/server --ignore tests",
    "pretest": "cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "factory-girl": "^5.0.4",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.2",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-plugin-transform-default-import": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "git-commit-msg-linter": "^2.3.11",
    "jest": "^24.9.0",
    "pre-commit-with-lint": "^1.2.5",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-default-import"
    ]
  },
  "pre-commit": {
    "run": [],
    "lint": {
      "stylelint": "false",
      "eslint": ".jsx?$"
    }
  }
}
